#!/usr/bin/env node

const dice = require("../lib/dice");

const [, , ...diceSets] = process.argv;

console.log(
  diceSets.map(set => {
    const [, count, sides] = set.match(/^(\d+)(d\d+)$/);

    /* eslint-disable-next-line security/detect-object-injection */
    return [set, dice[sides](parseInt(count))];
  })
);
